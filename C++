#include<iostream>
using namespace std;
int main()
{
  int x;
  int y;
  int t;
    cout<<"inter your first number \n";
    cin >>x;
    cout << "inter your second number" << endl;
    cin >>y;
    cout << "inter your jober(1=+,2=*,3=-,4=รท)" << endl;
    cin >>t;
    if (t==1)
    {cout << "the number is "<<y+x<< endl;}
    else if (t==2)
    {cout << "the number is "<<y*x<< endl;}
    else if (t==3)
    {cout << "the number is "<<x-y<< endl;}
    else if(t==4)
    {cout << "the number is "<<x/y<< endl;}
    else
    {cout << "error" << endl;}
    return 0;
}
